	<queryString>
		<![CDATA[SELECT 
  graduado.cedula, 
  graduado.id_especial, 
  especialidad.nomb_espec, 
  turno.nomb_turno, 
  turno."Id",
  graduado.fec_graduacion, 
  graduado.libro, 
  graduado.tomo, 
  graduado.folio, 
  graduado.numero, 
  graduado.resolucion, 
  graduado.fec_acta, 
  graduado.fec_firma, 
  graduado.reg_grado, 
  graduado.aranceles, 
  graduado.sede, 
  alumno.apellidos, 
  alumno.nombres, 
  alumno.nacionalidad, 
  alumno_materia.cod_mat, 
  alumno_materia.nota, 
  alumno_materia.fecha, 
  materia.descrip_mat, 
  materia_espec.uc, 
  CASE 
	WHEN (turno."Id" = 4) THEN materia_espec.termino
	ELSE materia_espec.semestre
  END AS semter,
  horario_grupo.nombre_lapso,
  materia_espec.semestre, 
  materia_espec.termino,
  CASE WHEN alumno_materia.nota='AP' THEN 0
       WHEN alumno_materia.nota='RE' THEN 0
       WHEN fun_nota_aprobada(1,alumno_materia.nota)=false THEN 0
       ELSE alumno_materia.nota::Integer
  END AS nota_int,
  CASE WHEN alumno_materia.nota='10' THEN 'DIEZ'
       WHEN alumno_materia.nota='11' THEN 'ONCE'
       WHEN alumno_materia.nota='12' THEN 'DOCE'
       WHEN alumno_materia.nota='13' THEN 'TRECE'
       WHEN alumno_materia.nota='14' THEN 'CATORCE'
       WHEN alumno_materia.nota='15' THEN 'QUINCE'
       WHEN alumno_materia.nota='16' THEN 'DIECISEIS'
       WHEN alumno_materia.nota='17' THEN 'DIECISIETE'
       WHEN alumno_materia.nota='18' THEN 'DIECIOCHO'
       WHEN alumno_materia.nota='19' THEN 'DIECINUEVE'
       WHEN alumno_materia.nota='20' THEN 'VEINTE'
       WHEN alumno_materia.nota='AP' THEN 'APROBADO'
       ELSE 'REPROBADO'
  END AS nota_letra,
  CASE WHEN fun_nota_aprobada(1,alumno_materia.nota) THEN materia_espec.uc
       ELSE 0
  END AS uc_aprob
FROM 
  public.graduado, 
  public.alumno_materia, 
  public.especialidad, 
  public.turno, 
  public.alumno, 
  public.materia, 
  public.materia_espec,
  horario_grupo
WHERE 
  graduado.cedula = alumno_materia.cedula AND
  graduado.id_especial = especialidad."Id" AND
  graduado.id_turno = turno."Id" AND
  graduado.cedula = alumno.cedula AND
  alumno_materia.cod_mat = materia_espec.cod_mat AND
  alumno_materia.id_espec = materia_espec.id_espec AND
  materia.cod_mat = alumno_materia.cod_mat AND
  horario_grupo."Id"=turno.id_grupo_horario AND 

  fun_nota_aprobada(1,alumno_materia.nota)=true AND

  graduado.cedula = $P{CEDULA}::integer AND
  graduado.id_especial = $P{ESPECIALIDAD}::integer
ORDER BY semter;]]>
