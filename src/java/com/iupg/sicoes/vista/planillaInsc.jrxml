	<queryString>
		<![CDATA[SELECT
     alumno.cedula AS cedula,
     alumno.apellidos||', '||alumno.nombres AS apellidos_nombres,
     alumno.tel_hab AS tel_hab,
     alumno.tel_ofc AS tel_ofc,
     alumno.tel_cel AS tel_cel,
     alumno.id_usu AS id_usu,
     usuario.ident_correo AS ident_correo,
     usuario."Id" AS "Id",
     turno.nomb_turno AS nomb_turno,
     especialidad.nomb_espec AS nomb_espec,
     inscripcion_alumno.periodo AS periodo,
     to_char(inscripcion_alumno.fec_hor_act,'DD/MM/YYYY HH12:MI:SS AM') AS fec_hor_act,
     inscripcion_alumno_detalle.cod_mat AS cod_mat,
     inscripcion_alumno_detalle.id_mat_espec_aula_secc_tur AS id_mat_espec_aula_secc_tur,
     materia_espec_aula_seccion_turno.seccion AS seccion,
     materia_espec_aula_seccion_turno.sede AS sede,
     materia_espec_aula_seccion_turno.aula AS aula,
     materia.descrip_mat AS descrip_mat,
     materia_espec.uc AS uc,
     periodo.fec_ini AS fec_ini,
     horario.dia AS dia,
     MIN(horario.hora_ini) AS hora_ini,
     MAX(horario.hora_fin) AS hora_fin
FROM
     "public"."alumno",
     "public"."especialidad",
     "public"."turno",
     "public"."inscripcion_alumno",
     "public"."inscripcion_alumno_detalle",
     "public"."materia",
     "public"."materia_espec_aula_seccion_turno",
     "public"."usuario",
     "public"."materia_espec",
     "public"."periodo",
     "public"."horario",
     "public"."profesor_horario"
WHERE
     alumno.id_turno = turno."Id"
 AND alumno.id_especial = especialidad."Id"
 AND alumno.cedula = inscripcion_alumno.cedula
 AND inscripcion_alumno.cedula = inscripcion_alumno_detalle.cedula
 AND inscripcion_alumno.periodo = inscripcion_alumno_detalle.periodo
 AND inscripcion_alumno_detalle.id_mat_espec_aula_secc_tur = materia_espec_aula_seccion_turno."Id"
 AND usuario."Id" = alumno.id_usu
 AND inscripcion_alumno_detalle.cod_mat = materia.cod_mat
 AND materia_espec.cod_mat = materia.cod_mat
 AND materia_espec.id_espec = materia_espec_aula_seccion_turno.id_espec
 AND alumno.cedula = $P{CEDULA}
 AND inscripcion_alumno_detalle.periodo = $P{PERIODO}
 AND inscripcion_alumno_detalle.periodo = periodo.periodo
 AND inscripcion_alumno_detalle.estatus IN ('I','D')
 AND materia_espec_aula_seccion_turno."Id" = inscripcion_alumno_detalle.id_mat_espec_aula_secc_tur
 AND profesor_horario.id_mat_espec_aula_secc_tur = materia_espec_aula_seccion_turno."Id"
 AND horario."Id" = profesor_horario.id_horario
GROUP BY
     alumno.cedula,
     alumno.apellidos||', '||alumno.nombres,
     alumno.tel_hab,
     alumno.tel_ofc,
     alumno.tel_cel,
     alumno.id_usu,
     usuario.ident_correo,
     usuario."Id",
     turno.nomb_turno,
     especialidad.nomb_espec,
     inscripcion_alumno.periodo,
     to_char(inscripcion_alumno.fec_hor_act,'DD/MM/YYYY HH12:MI:SS AM'),
     inscripcion_alumno_detalle.cod_mat,
     inscripcion_alumno_detalle.id_mat_espec_aula_secc_tur,
     materia_espec_aula_seccion_turno.seccion,
     materia_espec_aula_seccion_turno.sede,
     materia_espec_aula_seccion_turno.aula,
     materia.descrip_mat,
     materia_espec.uc,
     periodo.fec_ini,
     horario.dia
ORDER BY
     alumno.cedula ASC,
     alumno.apellidos ASC,
     alumno.tel_hab ASC,
     alumno.tel_ofc ASC,
     alumno.tel_cel ASC,
     alumno.id_usu ASC,
     usuario.ident_correo ASC,
     usuario."Id" ASC,
     turno.nomb_turno ASC,
     especialidad.nomb_espec ASC,
     inscripcion_alumno.periodo ASC,
     to_char(inscripcion_alumno.fec_hor_act,'DD/MM/YYYY HH12:MI:SS AM') ASC,
     inscripcion_alumno_detalle.cod_mat ASC,
     inscripcion_alumno_detalle.id_mat_espec_aula_secc_tur ASC,
     materia_espec_aula_seccion_turno.seccion ASC,
     materia_espec_aula_seccion_turno.sede ASC,
     materia_espec_aula_seccion_turno.aula ASC,
     materia.descrip_mat ASC]]>
	</queryString>
