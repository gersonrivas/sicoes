	<queryString>
		<![CDATA[SELECT DISTINCT
  materia_espec_aula_seccion_turno.periodo, 
  materia_espec_aula_seccion_turno.cod_mat, 
  materia_espec_aula_seccion_turno.id_espec, 
  materia_espec_aula_seccion_turno.seccion, 
  materia_espec_aula_seccion_turno.id_turno,
  turno.nomb_turno,
  profesor_horario.cedula AS ced_prof,
  profesor.apellidos||', '||profesor.nombres AS apellidos_nombres_prof,
  materia.descrip_mat,
  inscripcion_alumno_detalle.cedula AS ced_alum, 
  alumno.apellidos||', '||alumno.nombres AS apellidos_nombres_alum
FROM 
  public.materia_espec_aula_seccion_turno 
INNER JOIN public.turno ON (turno."Id" = materia_espec_aula_seccion_turno.id_turno)
INNER JOIN profesor_horario ON (profesor_horario.id_mat_espec_aula_secc_tur = materia_espec_aula_seccion_turno."Id")
INNER JOIN profesor ON (profesor.cedula = profesor_horario.cedula)
INNER JOIN materia ON (materia.cod_mat = materia_espec_aula_seccion_turno.cod_mat)
INNER JOIN inscripcion_alumno_detalle ON (inscripcion_alumno_detalle.id_mat_espec_aula_secc_tur = materia_espec_aula_seccion_turno."Id")
INNER JOIN alumno ON (alumno.cedula = inscripcion_alumno_detalle.cedula)
WHERE profesor.cedula = $P{CEDULA}
AND materia_espec_aula_seccion_turno.periodo = $P{PERIODO}
AND inscripcion_alumno_detalle.cod_mat = $P{MATERIA}
AND materia_espec_aula_seccion_turno.seccion = $P{SECCION}
AND inscripcion_alumno_detalle.estatus IN ('I','D')
ORDER BY inscripcion_alumno_detalle.cedula;]]>
	</queryString>
