	<queryString>
		<![CDATA[SELECT 
  inscripcion_alumno.periodo, 
  alumno.cedula, 
  alumno.apellidos, 
  alumno.nombres, 
  inscripcion_alumno.estatus, 
  to_char(inscripcion_alumno.fec_hor_act, 'DD/MM/YYYY HH24:MI:SS') AS fec_hor_act,
  alumno.sede, 
  alumno.id_especial, 
  especialidad."Id", 
  especialidad.nomb_espec, 
  turno.nomb_turno, 
  alumno.id_turno
FROM 
  public.inscripcion_alumno, 
  public.alumno, 
  public.especialidad, 
  public.turno
WHERE 
  inscripcion_alumno.estatus = 'C' AND
  alumno.cedula = inscripcion_alumno.cedula AND
  alumno.id_especial = especialidad."Id" AND
  alumno.id_turno = turno."Id" AND
  alumno.sede = $P{SEDE} AND
  inscripcion_alumno.periodo = $P{PERIODO}
ORDER BY alumno.cedula;]]>
	</queryString>
